    <definitions>
      <definition>
        <class_type>TRANSFORM_CLASS</class_type>
        <class_name>Processor</class_name>
        <class_source>import java.util.*;
import java.lang.*;
import java.text.*;


String[] fieldNames;

HashMap fieldMap;

private ArrayList hdrFields = new ArrayList();


public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{

	fieldMap = new HashMap();

	String xml = "";
	if (first) { 
		first = false;

        RowSet hdrStream = findInfoRowSet("HDRFields");

        Object[] hdrRow = null;


        // Read all rows from info step before calling getRow() method, which returns first row from any
        // input rowset. As rowMeta for info and input steps varies getRow() can lead to errors.
        while((hdrRow = getRowFrom(hdrStream)) != null){
			hdrFields.add(hdrRow[0]);
        }
		xml += "&lt;?xml version=\"1.0\" encoding=\"ISO-8859-1\"?>\n";
		DateFormat df = new SimpleDateFormat("yyyy-MM-dd HH:mm:ss");
		Date dateobj = new Date();
		String currentDate = df.format(dateobj);
		xml += "&lt;AMS_DOC_XML_EXPORT_FILE VERSION=\"1.0\" EXPORT_DATE=\"" + currentDate + "\">\n";
	}

	Object[] r = getRow();
	if (r == null) {
		setOutputDone();
		return false;
    }


    String DOC_CAT = get(Fields.In, "DOC_CAT").getString(r);
    String DOC_TYP = get(Fields.In, "DOC_TYP").getString(r);
    String DOC_CD = get(Fields.In, "DOC_CD").getString(r);
    String DOC_DEPT_CD = get(Fields.In, "DOC_DEPT_CD").getString(r);
    String DOC_ID = get(Fields.In, "DOC_ID").getString(r);  
    String DOC_VERS_NO = get(Fields.In, "DOC_VERS_NO").getString(r);
    String DOC_IMPORT_MODE = get(Fields.In, "DOC_IMPORT_MODE").getString(r);
	String AUTO_DOC_NUM = get(Fields.In, "AUTO_DOC_NUM").getString(r); 
    String EMPLOYEE_ID = get(Fields.In, "EMPLOYEE_ID").getString(r);
    String EMPL_SSN = get(Fields.In, "EMPL_SSN").getString(r);
	String APP_SSN_FL = get(Fields.In, "APP_SSN_FL").getString(r);
    String BIRTH_DT = get(Fields.In, "BIRTH_DT").getString(r);
    String CITIZENSHIP_ID = get(Fields.In, "CITIZENSHIP_ID").getString(r);
    String ETHN_CD = get(Fields.In, "ETHN_CD").getString(r);
    String SEX_ID = get(Fields.In, "SEX_ID").getString(r);
    String VETERAN_STA_CD = get(Fields.In, "VETERAN_STA_CD").getString(r);
    String DISCHARGE_TYP_CD = get(Fields.In, "DISCHARGE_TYP_CD").getString(r);
    String MILITARY_BRANCH_CD = get(Fields.In, "MILITARY_BRANCH_CD").getString(r);
	String MILITARY_RANK_CD = get(Fields.In, "MILITARY_RANK_CD").getString(r);
    String DEATH_DT = get(Fields.In, "DEATH_DT").getString(r);
    String GEND_CD = get(Fields.In, "GEND_CD").getString(r);
	String ApplicantUserGUID = get(Fields.In, "ApplicantUserGUID").getString(r);


	
	String DEPT = null;
	String UNIT = null;

	String RequisitionDivisionID = get(Fields.In, "RequisitionDivisionID").getString(r);
	if(RequisitionDivisionID != null){
	    if(RequisitionDivisionID.contains("-")){
	    	String[] divisions =  RequisitionDivisionID.split("-");
		    DEPT = divisions[0];
		    UNIT = divisions[1];
		}else{
			DEPT = RequisitionDivisionID;
		}
	}


    //Add Shared Fields	
    addToFieldMap("DOC_CAT", DOC_CAT);
    addToFieldMap("DOC_TYP", DOC_TYP);
    addToFieldMap("DOC_CD", DOC_CD);	
    addToFieldMap("DOC_DEPT_CD", DEPT);	
    addToFieldMap("DOC_ID", DOC_ID);
    addToFieldMap("DOC_VERS_NO", DOC_VERS_NO);
    addToFieldMap("EMPLOYEE_ID", EMPLOYEE_ID);
    addToFieldMap("EMPL_SSN", EMPL_SSN);
	addToFieldMap("APP_SSN_FL", APP_SSN_FL);
    addToFieldMap("BIRTH_DT", BIRTH_DT);
//	addToFieldMap("CITIZENSHIP_ID", CITIZENSHIP_ID);
	addToFieldMap("ETHN_CD", ETHN_CD);
//	addToFieldMap("SEX_ID", SEX_ID);
//	addToFieldMap("VETERAN_STA_CD", VETERAN_STA_CD);
//	addToFieldMap("DISCHARGE_TYP_CD", DISCHARGE_TYP_CD);
//	addToFieldMap("MILITARY_BRANCH_CD", MILITARY_BRANCH_CD);
//	addToFieldMap("MILITARY_RANK_CD", MILITARY_RANK_CD);
//	addToFieldMap("DEATH_DT", DEATH_DT);
	addToFieldMap("GEND_CD", GEND_CD);
	addToFieldMap("USER_TXT_1", ApplicantUserGUID);


 	
    xml += "&lt;AMS_DOCUMENT DOC_CAT=\"" + DOC_CAT + 
			"\" DOC_TYP=\"" + DOC_TYP + 
			"\" DOC_CD=\"" + DOC_CD + 
			"\" DOC_DEPT_CD=\"" + DEPT + 
			//"\" DOC_UNIT_CD=\"" + fieldMap.get("DOC_UNIT_CD") + 
			"\" DOC_ID=\"" + fieldMap.get("DOC_ID") + 
			"\" DOC_VERS_NO=\"" + DOC_VERS_NO + 
			"\" AUTO_DOC_NUM=\"" + AUTO_DOC_NUM + 
			"\" DOC_IMPORT_MODE=\"" + DOC_IMPORT_MODE + "\">\n";
    // Print HDRDR AMSD
    xml += "\t&lt;ATTR_DOC_HDR AMSDataObject=\"Y\">\n";
    xml += printFields(hdrFields); 
    xml += "\t&lt;/ATTR_DOC_HDR>";
    //End HDR 

    xml += "\n&lt;/AMS_DOCUMENT>";
    r = createOutputRow(r, data.outputRowMeta.size());
    get(Fields.Out, "XMLLine").setValue(r, xml);
    // Send the row on to the next step.
    putRow(data.outputRowMeta, r);
	    

    return true;

}


private void print(String s){
	logBasic(s);
}

private String printXML(String field, String element){
	String retVal = "\t\t\t&lt;" + field + " Attribute=\"Y\">&lt;![CDATA[" + element.toString() + "]]" +">&lt;/" + field + ">\n";
	return retVal;
}

private String printFields(ArrayList fields){
	String xml = "";
	for(int i = 0; i &lt; fields.size(); i++){
			String fieldName = (String) fields.get(i);
			Object temp = fieldMap.get(fieldName);
			if(temp == null){
				xml += printXML(fieldName, "null");
			}else{
				xml += printXML(fieldName, (String) fieldMap.get(fieldName));
			}
		}
	return xml;

}
 
private String printFields(String[] fields){
	String xml = "";
	for(int i = 0; i &lt; fields.length; i++){
			Object temp = fieldMap.get(fields[i]);
			if(temp == null){
				xml += printXML(fields[i], "null");
			}else{
				xml += printXML(fields[i], (String) fieldMap.get(fields[i]));
			}
		}
	return xml;
}


private void addToFieldMap(String key, String value){
	fieldMap.put(key, value);
}
